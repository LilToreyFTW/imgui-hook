{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build Loader",
            "type": "shell",
            "command": "msbuild",
            "args": [
                "${workspaceFolder}/Loader/Loader.sln",
                "/p:Configuration=Release",
                "/p:Platform=x64",
                "/v:minimal"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent",
                "panel": "shared"
            },
            "problemMatcher": ["$msCompile"]
        }
    ]
}
